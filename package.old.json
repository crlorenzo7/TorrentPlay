        "morgan": "~1.5.3",
        "nedb": "*",
        "nodecast-js": "^1.0.3",
        "peerflix": "*",
        "pump": "^1.0.1",
        "torrent-stream": "*",
        "unzip": "*",
        "upnp-mediarenderer-client": "*",
        "vlc-simple-player": "^0.5.0",
